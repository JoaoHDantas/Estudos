
d1 = input('Digite alguma coisa para saber seus tipos primitivos: ')
print(type(d1))
print('Ele é numerico?',d1.isnumeric())
print('Ele é alfabetico?',d1.isalpha())
print('Ele é alfanumerico?',d1.isalnum())
print('Ele está em minúsculo?',d1.islower())
print('Ele está em MAIÚSCULO?',d1.isupper())
print('Ele está captilizada',d1.istitle())
print('Ele é um espaço?',d1.isspace())
print('Ele pode ser impresso?',d1.isprintable())
print('Ele é decimal',d1.isdecimal())
